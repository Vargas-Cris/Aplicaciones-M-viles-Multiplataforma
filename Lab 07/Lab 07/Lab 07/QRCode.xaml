<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Lab_07.QRCode">
    <ContentPage.Content>
        <StackLayout Spacing="10">
            <Button x:Name="btnQrCode" BackgroundColor="YellowGreen" Clicked="btnQrCode_Clicked" Text="QR Code" />
            <Entry x:Name="txtQrCode" Placeholder="Result from QR Scan"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>